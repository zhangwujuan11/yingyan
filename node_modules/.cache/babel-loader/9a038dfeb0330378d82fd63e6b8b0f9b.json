ActivationState,\n          Avatar: item.Avatar,\n          CreateAt: item.CreateAt,\n          ID: item.ID,\n          Nickname: item.Nickname,\n          OrgId: item.OrgId,\n          role: item.RoleId,\n          Status: item.Status,\n          UpdateAt: item.UpdateAt,\n          Username: item.Username\n        });\n      }); // console.warn(that.ahgg)\n    }); // 申请激活列表\n\n    adminList(JSON.parse(localStorage.getItem('urlUserinfor'))).then(function (res) {\n      _this.applylist = res.data.data; // console.log(this.applylist)\n    }); //获取身份标识\n\n    identity().then(function (res) {\n      _this.actions = res.data.data;\n    });\n  },\n  methods: {\n    addmember: function addmember() {\n      this.popupVisible = true;\n    },\n    // 关闭邀请表单\n    changVisible: function changVisible() {\n      this.popupVisible = false;\n    },\n    //不通过申请\n    fail: function fail(n) {\n      var _this2 = this;\n\n      var data = {\n        \"username\": n.Username,\n        \"org_id\": n.OrgId,\n        \"nickname\": n.Nickname,\n        \"role_id\": n.RoleIid,\n        \"avatar\": n.Avatar,\n        \"status\": n.Status,\n        \"activation_state\": 0\n      };\n      ActivationState(data).then(function (res) {\n        Toast('操作成功');\n\n        _this2.reload();\n      });\n    },\n    // 通过申请\n    pass: function pass(n) {\n      this.sendadform = {\n        \"username\": n.Username,\n        \"org_id\": n.OrgId,\n        \"nickname\": n.Nickname,\n        \"avatar\": n.Avatar,\n        \"status\": n.Status,\n        \"activation_state\": 1\n      };\n      this.ible = true;\n    },\n    passs: function passs() {\n      var _this3 = this;\n\n      this.sendadform.role_id = this.sheetVisible;\n      ActivationState(this.sendadform).then(function (res) {\n        _this3.ible = false;\n        Toast('操作成功');\n\n        _this3.reload();\n      });\n    },\n    // 删除成员\n    delpoeple: function delpoeple(n) {\n      var _this4 = this;\n\n      this.sendadform = {\n        \"use